{
  "name": "appscript-test",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Alex Kelley <https://github.com/adkelley",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist node_modules output .spago",
    "copy": "cp src/appsscript.json dist/",
	"move": "mv dist/appsscript.json src/",
	"bundle": "spago bundle-module --main Main --to bundle/index.js",
	"clasp:reset": "rm .clasp.json src/appsscript.json",
    "clasp:create": "clasp create --rootDir ./dist && yarn move",
    "build": "yarn bundle && rimraf dist && webpack --mode production && yarn copy",
    "push": "clasp push",
    "watch": "watch 'yarn push' dist/"
  },
  "devDependencies": {
    "clasp": "^1.0.0",
    "gas-webpack-plugin": "^1.0.4",
    "webpack": "^4.44.1",
    "watch": "^1.0.2",
    "webpack-cli": "^3.3.12"
  }
}
